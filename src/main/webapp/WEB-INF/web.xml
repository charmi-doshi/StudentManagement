<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServController</servlet-name>
  	<display-name>LoginServController</display-name>
  	<description></description>
  	<servlet-class>com.controller.LoginServController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentCont</servlet-name>
  	<display-name>StudentCont</display-name>
  	<description></description>
  	<servlet-class>com.controller.StudentCont</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServController</servlet-name>
  	<url-pattern>/LoginServController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentCont</servlet-name>
  	<url-pattern>/StudentCont</url-pattern>
  </servlet-mapping>
</web-app>
